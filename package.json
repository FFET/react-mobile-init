{
  "name": "react-mobile-kit",
  "version": "0.1.5",
  "private": true,
  "description": "react mobile kit",
  "keywords": [
    "react",
    "redux",
    "react-router"
  ],
  "license": "MIT",
  "author": {
    "name": "Jay",
    "email": "jay.wang.scorpion@gmail.com",
    "url": "https://scorpionjay.github.io/blog"
  },
  "scripts": {
    "build": "npm run build:prod",
    "build:pr": "cross-env NODE_ENV=production BUILD_ENV=pr webpack --config build/webpack.prod.config.js",
    "build:prod": "cross-env NODE_ENV=production BUILD_ENV=prod webpack --config build/webpack.prod.config.js",
    "build:sit": "cross-env NODE_ENV=production BUILD_ENV=sit webpack --config build/webpack.prod.config.js",
    "build:uat": "cross-env NODE_ENV=production BUILD_ENV=uat webpack --config build/webpack.prod.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "dll": "webpack --config build/webpack.dll.config.js -p --progress --profile",
    "lint": "npm run lint:js & npm run lint:css",
    "lint-staged": "lint-staged",
    "lint:css": "stylelint --fix 'src/**/*.scss'",
    "lint:js": "eslint --fix src",
    "prettier": "prettier --write  \"./**/*.{js,jsx,css,scss,md,json}\"",
    "sort": "npx sort-package-json",
    "deploy": "node ./scripts/deploy/deploy.js",
    "start": "npm run dev",
    "test": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*{js,jsx}": [
      "prettier --write  \"./**/*.{js,jsx,css,scss,md,json}\"",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "antd-mobile": "^2.3.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "fastclick": "^1.0.6",
    "moment": "^2.24.0",
    "rc-form": "^2.4.11",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime-corejs3": "^7.8.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/react": "^16.9.23",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "cloud-storage-webpack-plugin": "^0.0.5",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eruda": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "husky": "^4.2.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-ssh": "^10.0.2",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "1.19.1",
    "react-hot-loader": "4.12",
    "redux-logger": "^3.0.6",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-scss": "^3.15.0",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "zip-webpack-plugin": "^3.0.0"
  }
}

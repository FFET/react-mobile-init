{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@config/*": ["config/*"],
      "@page/*": ["src/page/*"],
      "@components/*": ["src/components/*"],
      "@contexts/*": ["src/contexts/*"],
      "@utils/*": ["src/utils/*"],
      "@static/*": ["src/static/*"],
      "@stories/*": ["src/stories"],
      "@biz/*": ["src/bizComponents/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
